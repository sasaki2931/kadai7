<%= form_with(model: Share.new, local: true) do |form| %>
    <div class="share_title">
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div class="share_content">
      <%= form.label :content %>
      <%= form.text_field :content %>
    </div>
    <%= form.submit %>
  <% end %>